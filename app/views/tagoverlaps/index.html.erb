<p id="notice"><%= notice %></p>

<h1>Listing Tagoverlaps</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Name</th>
      <th>Tagtarget</th>
      <th>Nametarget</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tagoverlaps.each do |tagoverlap| %>
      <tr>
       
        <td><%= tagoverlap.name %></td>
      
        <td><%= tagoverlap.nametarget %></td>
        <td><%= link_to 'Show', tagoverlap %></td>
        <td><%= link_to 'Edit', edit_tagoverlap_path(tagoverlap) %></td>
        <td><%= link_to 'Destroy', tagoverlap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tagoverlap', new_tagoverlap_path %>
